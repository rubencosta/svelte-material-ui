  "description": "Svelte Material UI - Text Field",
  "keywords": [
    "svelte",
    "svelte3",
    "material-ui",
    "material-design",
    "material",
    "svelte-components",
    "sveltejs"
  ],
  "scripts": {
    "prepublishOnly": "echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map _style.scss bare.css\" | xargs sass && echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map character-counter/_style.scss character-counter/bare.css\" | xargs sass && echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map helper-text/_style.scss helper-text/bare.css\" | xargs sass && echo \"$(npx glob '**/*' | sed 's/^/-I /' | paste -s -d ' ' -) --no-source-map icon/_style.scss icon/bare.css\" | xargs sass",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hperrin/svelte-material-ui.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "bugs": {
    "url": "https://github.com/hperrin/svelte-material-ui/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@material/feature-targeting": "^3.1.0",
    "@material/ripple": "^3.2.0",
    "@material/rtl": "^3.2.0",
    "@material/textfield": "^3.2.0",
    "@smui/common": "https://gitpkg.now.sh/rubencosta/svelte-material-ui/packages/common?6604bf427a2ca22e7fd07dd322a512398e71e1c9",
    "@smui/floating-label": "https://gitpkg.now.sh/rubencosta/svelte-material-ui/packages/floating-label?6604bf427a2ca22e7fd07dd322a512398e71e1c9",
    "@smui/line-ripple": "https://gitpkg.now.sh/rubencosta/svelte-material-ui/packages/line-ripple?6604bf427a2ca22e7fd07dd322a512398e71e1c9",
    "@smui/notched-outline": "https://gitpkg.now.sh/rubencosta/svelte-material-ui/packages/notched-outline?6604bf427a2ca22e7fd07dd322a512398e71e1c9"
  },
  "devDependencies": {
    "glob-cmd": "0.0.7",
    "sass": "^1.25.0"
  }
